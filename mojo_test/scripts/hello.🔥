from python import Python

struct MyPair:
  var first: Int
  var second: Int
  fn __init__(inout self, first: Int, second: Int):
    self.first = first
    self.second = second
  fn dump(self):
    print(self.first, self.second)
    
fn main():

  fn do_math():
    let x: Int = 1
    let y = 2
    print(x + y)
    
  fn add(x: Int, y: Int) -> Int:
    return x + y
    
  fn pow(base: Int, exp: Int = 2) -> Int:
    return base ** exp
    
  fn add_borrowed(borrowed x: Int, borrowed y: Int) -> Int:
    return x + y
    
  fn add_inout(inout x: Int, inout y: Int) -> Int:
    x += 1
    y += 1
    return x + y
    
  fn set_fire(owned text: String) -> String:
    text += "ðŸ”¥"
    return text 
  fn mojo():
    let a: String = "mojo"
    let b = set_fire(a)
    print(a)
    print(b)

  let mine = MyPair(2, 4)
  mine.dump()
  try:
    let np = Python.import_module("numpy")
    let ar = np.arange(15).reshape(3, 5)
    print(ar)
    print(ar.shape)
  except:
    print("error")
  finally:
    print("done")
